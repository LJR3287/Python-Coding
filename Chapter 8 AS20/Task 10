def reverse(in_str):
    out_str=""
    i=len(in_str)-1
    while i>=0:
        out_str+=in_str[i]
        i-=1
    return out_str

def is_palindrome(str1):
    str2=reverse(str1)
    return (str2==str1)

print ("abba", is_palindrome("abba"))
print ("abab", is_palindrome("abab"))
print ("tenet",is_palindrome("tenet"))
print ("banana", is_palindrome("banana"))
print ("straw warts",is_palindrome("straw warts"))
print ("a", is_palindrome("a"))
print ("", is_palindrome("")) 
